  "scripts": {
    "lint": "tslint --project ./",
    "global": "npm install -g standard-version commitizen yarn nyc jest husky",
    "test": "jest --verbose --watchAll --env=jsdom",
    "test:integration": "jest --watchAll=false --verbose --env=jsdom  --runInBand --coverage --testResultsProcessor ./node_modules/jest-html-reporter",
