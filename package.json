{
  "name": "ui-kit",
  "version": "0.1.0",
  "main": "dist/index.js",
  "private": true,
  "scripts": {
    "lint": "./node_modules/.bin/tslint --project ./",
    "global": "npm install -g yarn nyc react-scripts-ts",
    "test:local": "react-scripts test --verbose --watchAll --env=jsdom",
    "test": "SKIP_PREFLIGHT_CHECK=true react-scripts test --watchAll=false --verbose --env=jsdom --coverage --testResultsProcessor ./node_modules/jest-html-reporter",
    "build": "react-scripts build",
    "eject": "react-scripts eject",
    "start": "start-storybook -p 6006 -c .storybook",
    "build-storybook": "build-storybook"
  },
  "eslintConfig": {
    "extends": "react-app"
  },
  "browserslist": {
    "production": [
      ">0.2%",
      "not dead",
      "not op_mini all"
    ],
    "development": [
      "last 1 chrome version",
      "last 1 firefox version",
      "last 1 safari version"
    ]
  },
  "dependencies": {
    "bootstrap": "^4.3.1",
    "formik": "^1.5.8",
    "react": "^16.8.6",
    "react-dom": "^16.8.6",
    "react-scripts": "3.0.1",
    "reactstrap": "^8.0.1",
    "styled-components": "^4.3.2",
    "to-string-loader": "^1.1.5",
    "typescript": "^3.5.3"
  },
  "devDependencies": {
    "@babel/core": "^7.5.5",
    "@commitlint/cli": "^8.1.0",
    "@commitlint/config-conventional": "^8.1.0",
    "@storybook/addon-actions": "^5.1.10",
    "@storybook/addon-info": "^5.1.10",
    "@storybook/addon-knobs": "^5.1.10",
    "@storybook/addon-links": "^5.1.10",
    "@storybook/addons": "^5.1.10",
    "@storybook/react": "^5.1.10",
    "@types/chai": "^4.1.7",
    "@types/enzyme": "^3.10.3",
    "@types/enzyme-adapter-react-16": "^1.0.5",
    "@types/jest": "^24.0.16",
    "@types/node": "12.6.8",
    "@types/react": "16.8.23",
    "@types/react-dom": "16.8.5",
    "@types/reactstrap": "^8.0.1",
    "@types/sinon": "^7.0.13",
    "@types/storybook__react": "^4.0.2",
    "@types/styled-components": "^4.1.18",
    "awesome-typescript-loader": "^5.2.1",
    "babel-loader": "^8.0.6",
    "babel-plugin-macros": "^2.6.1",
    "babel-plugin-require-context-hook": "^1.0.0",
    "chai": "^4.2.0",
    "css-loader": "^3.1.0",
    "enzyme": "^3.10.0",
    "enzyme-adapter-react-16": "^1.14.0",
    "fork-ts-checker-webpack-plugin": "^1.4.3",
    "husky": "^3.0.2",
    "jest": "^24.8.0",
    "jest-html-reporter": "^2.5.0",
    "postcss-loader": "^3.0.0",
    "react-docgen-typescript": "^1.12.5",
    "react-docgen-typescript-loader": "^3.1.0",
    "react-docgen-typescript-webpack-plugin": "^1.1.0",
    "require-context.macro": "^1.1.0",
    "sinon": "^7.3.2",
    "style-loader": "^0.23.1",
    "ts-jest": "^24.0.2",
    "ts-node": "^8.3.0",
    "tslint": "^5.18.0",
    "tslint-config-prettier": "^1.18.0",
    "tslint-react": "^4.0.0",
    "url-loader": "^2.1.0"
  },
  "jest": {
    "coverageReporters": [
      "lcov",
      "text-summary",
      "html"
    ],
    "collectCoverageFrom": [
      "src/**/*.tsx",
      "!src/**/*.stories.tsx",
      "!src/**/*.test.tsx"
    ]
  }
}
